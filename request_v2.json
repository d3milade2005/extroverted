        "note": "Replace FASHION_CATEGORY_UUID with actual UUID from step 1"
      },
      {
        "step": 5,
        "action": "Get event details",
        "request": "8_getEventById",
        "note": "Replace EVENT_UUID with UUID from step 2"
      },
      {
        "step": 6,
        "action": "Find nearby events",
        "request": "12_findNearbyEvents",
        "expected": "Should return events created in steps 2-4"
      },
      {
        "step": 7,
        "action": "Save event",
        "request": "24_saveEvent",
        "note": "Save the music event from step 2"
      },
      {
        "step": 8,
        "action": "RSVP to event",
        "request": "25_rsvpEvent",
        "note": "RSVP to the music event"
      },
      {
        "step": 9,
        "action": "Get my events",
        "request": "22_getMyEvents",
        "expected": "Should show all events you created"
      },
      {
        "step": 10,
        "action": "Search free events",
        "request": "14_searchFreeEvents",
        "expected": "Should show the free fitness bootcamp"
      },
      {
        "step": 11,
        "action": "Update event",
        "request": "9_updateEvent",
        "note": "Update the music event price"
      },
      {
        "step": 12,
        "action": "Complex search",
        "request": "21_complexSearch",
        "expected": "Filtered results based on multiple criteria"
      }
    ]
