      "categories": {
        "list": "/api/categories",
        "getById": "/api/categories/{id}",
        "getByName": "/api/categories/name/{name}"
      },
      "events": {
        "create": "/api/events",
        "getById": "/api/events/{id}",
        "update": "/api/events/{id}",
        "delete": "/api/events/{id}",
        "upcoming": "/api/events/upcoming",
        "nearby": "/api/events/nearby",
        "search": "/api/events/search",
        "myEvents": "/api/events/host/me",
        "byCategory": "/api/events/category/{categoryId}"
      },
      "interactions": {
        "create": "/api/events/{id}/interactions",
        "remove": "/api/events/{id}/interactions/{type}"
      }
    }
  },
