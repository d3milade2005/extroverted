  "testScenario": {
    "description": "Complete workflow for testing event service",
    "steps": [
      {
        "step": 1,
        "action": "Get categories",
        "request": "1_getCategories",
        "saveResponse": "Save category UUIDs for creating events"
      },
      {
        "step": 2,
        "action": "Create music event",
        "request": "2_createMusicEvent",
        "note": "Replace MUSIC_CATEGORY_UUID with actual UUID from step 1",
        "saveResponse": "Save event UUID"
      },
      {
        "step": 3,
        "action": "Create tech event",
        "request": "3_createTechEvent",
